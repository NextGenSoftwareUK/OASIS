{
  "name": "@oasis/webui-devkit-svelte",
  "version": "1.0.0",
  "description": "OASIS Web UI Development Kit for Svelte",
  "type": "module",
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.spec.*"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
  },
  "keywords": [
    "oasis",
    "svelte",
    "web3",
    "components"
  ],
  "author": "OASIS Network",
  "license": "MIT",
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "dependencies": {
    "@oasis/api-client": "^2.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.20.0",
    "@sveltejs/package": "^2.0.0",
    "publint": "^0.1.9",
    "svelte": "^4.0.0",
    "svelte-check": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2"
  }
}

