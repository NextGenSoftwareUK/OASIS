<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
	  <PackageId>NextGenSoftware.OASIS.API.Providers.TelegramOASIS</PackageId>
	  <Company>NextGen Software Ltd</Company>
	  <Product>WEB4 OASIS API Telegram OASIS Provider</Product>
	  <Title>WEB4 OASIS API Telegram OASIS Provider</Title>
