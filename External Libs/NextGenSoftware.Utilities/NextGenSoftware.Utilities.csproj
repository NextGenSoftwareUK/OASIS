<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
	<GeneratePackageOnBuild>True</GeneratePackageOnBuild>
	<PackageId>NextGenSoftware.Utilities</PackageId>
	<Company>NextGen Software Ltd</Company>
	<Product>NextGen Software Utilities</Product>
	<Title>NextGen Software Utilities</Title>
	<Summary>Utilities library used by other NextGen Software libraries.</Summary>
	<Description>Utilities library used by other NextGen Software libraries.</Description>
	<PackageProjectUrl>https://github.com/NextGenSoftwareUK/Our-World-OASIS-API-HoloNET-HoloUnity-And-.NET-HDK</PackageProjectUrl>
	<PackageIcon>STARJPG.JPG</PackageIcon>
	<Authors>David Ellams (NextGen Software Ltd)</Authors>
	<PackageTags>utilities;net;unity</PackageTags>
	<PackageReleaseNotes>
		- Upgraded to .NET 8.
		- Added ObjectToByteArray function to DataHelper.
		- Fixed bugs in ConvertBinaryDataToString &amp; DecodeBinaryDataAsUTF8 functions in DataHelper.
		- Added new ExpandoObjectHelpers.
	</PackageReleaseNotes>
	<RepositoryType>git</RepositoryType>
	<RepositoryUrl>https://github.com/NextGenSoftwareUK/Our-World-OASIS-API-HoloNET-HoloUnity-And-.NET-HDK</RepositoryUrl>
	<Copyright>Copyright Â© NextGen Software Ltd 2022 - 2024</Copyright>
	<PackageReadmeFile>README.md</PackageReadmeFile>
	<!--<PackageLicenseExpression>GNU GPLv3</PackageLicenseExpression>-->
	<PackageLicenseExpression>MIT</PackageLicenseExpression>
	<PackageRequireLicenseAcceptance>True</PackageRequireLicenseAcceptance>
	<Version>1.2.1</Version>
	<!--<PackageLicenseFile>..\LICENSE</PackageLicenseFile>-->
  </PropertyGroup>

  <ItemGroup>
    <None Include="STARJPG.JPG">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

  <ItemGroup>
    <None Update="README.md">
      <PackagePath>\</PackagePath>
      <Pack>True</Pack>
    </None>
  </ItemGroup>

</Project>
