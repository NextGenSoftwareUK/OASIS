# BillionsHealed Backend Environment Variables
# Copy this file to .env and fill in your values

# Server Configuration
PORT=3002
NODE_ENV=production

# Twitter API Configuration (Optional - for live Twitter integration)
TWITTER_BEARER_TOKEN=your_twitter_bearer_token_here
TWITTER_API_KEY=your_twitter_api_key_here
TWITTER_API_SECRET=your_twitter_api_secret_here

# CORS Configuration
CORS_ORIGIN=https://billionshealed.com

# Database Configuration (if using a database in the future)
# DATABASE_URL=your_database_url_here

# OASIS Integration (if using OASIS blockchain)
# OASIS_RPC_URL=your_oasis_rpc_url_here
# OASIS_PRIVATE_KEY=your_oasis_private_key_here

