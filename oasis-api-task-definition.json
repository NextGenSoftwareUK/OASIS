{
  "family": "oasis-api-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "512",
  "memory": "1024",
  "executionRoleArn": "arn:aws:iam::881490134703:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::881490134703:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "oasis-api-container",
      "image": "881490134703.dkr.ecr.us-east-1.amazonaws.com/oasis-api@sha256:179026ff5404c86f135d83120f842418b7749e0bfb7b01b7dd8d7fc8892f21fe",
      "cpu": 512,
      "memory": 1024,
